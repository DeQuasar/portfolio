{
  "extends": [
    "html-validate:document",
    "html-validate:recommended",
    "html-validate:standard"
  ],
  "rules": {
    "doctype-style": "error",
    "void-style": "error",
    "prefer-native-element": "error",
    "element-permitted-content": "error",
    "no-inline-style": "off",
    "require-sri": "off",
    "valid-id": "off",
    "no-redundant-role": "error",
    "no-dup-class": "error"
  }
}
